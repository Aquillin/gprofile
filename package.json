{
    "name": "gprofile",
    "version": "1.0.0",
    "description": "A CLI for storing and switching between user profiles in git repositories.",
    "main": "production/main.js",
    "bin": "production/main.js",
    "author": "Aquillin",
    "keywords": [
        "git",
        "user.name",
        "user.email",
        "github",
        "gprofile",
        "git profiles",
        "ssh"
    ],
    "license": "MIT",
    "private": false,
    "scripts": {
        "dev": "nodemon src/main.ts",
        "gprofile": "node dist/main.js",
        "build:dev": "tsc",
        "build:production": "tsc --outDir ./production",
        "relink": "npm unlink && npm link",
        "test": "jest"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "commander": "^7.2.0",
        "fs-extra": "^10.0.0",
        "inquirer": "^8.0.0",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-typescript": "^7.13.0",
        "@types/commander": "^2.12.2",
        "@types/fs-extra": "^9.0.10",
        "@types/inquirer": "^7.3.1",
        "@types/jest": "^26.0.22",
        "@types/lodash": "^4.14.168",
        "@types/node": "^14.14.37",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Aquillin/gprofile.git"
    },
    "bugs": {
        "url": "https://github.com/Aquillin/gprofile/issues"
    },
    "homepage": "https://github.com/Aquillin/gprofile#readme"
}
